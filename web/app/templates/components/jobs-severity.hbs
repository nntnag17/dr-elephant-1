<ul id="job_severities" class="severity-horizontal-list horizontal-list">
  {{#each jobsseverity as |severity|}}
      <li>
          <p class="severity-count" style="color:{{getBootstrapSeverityCode severity.severity}}">{{severity.count}}</p>
          <p class="scnd-font-color severity-{{severity.severity}}">{{severity.severity}}</p>
      </li>
  {{/each}}
</ul>